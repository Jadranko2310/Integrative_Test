<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="FE Test Suite" parallel="tests" thread-count="2">

    <!-- Functional Tests -->
    <test name="Test Suite 1">
        <classes>
            <class name="ui.AdminFunctionalTest"/>
            <class name="ui.UserFunctionalTest"/>
        </classes>
    </test>

    <!-- Negative Tests -->
    <test name="Test Suite 2">
        <classes>
            <class name="ui.AdminNegativeTests"/>
            <class name="ui.UserNegativeTests"/>
        </classes>
    </test>

</suite>
